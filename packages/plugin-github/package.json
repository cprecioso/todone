{
  "name": "@todone/plugin-github",
  "version": "0.7.2",
  "type": "module",
  "description": "A todone plugin that will alert you when an GitHub issue or pull request has been resolved",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cprecioso/todone.git"
  },
  "homepage": "https://github.com/cprecioso/todone/tree/main/packages/plugin-github#readme",
  "bugs": {
    "url": "https://github.com/cprecioso/todone/issues"
  },
  "author": "Carlos Precioso <npm@precioso.design>",
  "license": "ISC",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "check": "tsc --noEmit -p .",
    "dev": "tsdown --watch"
  },
  "dependencies": {
    "@octokit/auth-token": "^6.0.0",
    "@todone/internal-urlpattern": "workspace:^",
    "@todone/plugin": "workspace:^",
    "octokit": "^5.0.3",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@todone/internal-build": "workspace:*",
    "tsdown": "^0.13.0",
    "typescript": "~5.8.3"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "sideEffects": false
}
