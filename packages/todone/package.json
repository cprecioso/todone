{
  "name": "todone",
  "version": "0.8.0",
  "author": "Carlos Precioso <npm@precioso.design>",
  "license": "ISC",
  "packageManager": "yarn@3.5.0",
  "engines": {
    "node": ">=18"
  },
  "bin": "bin.mjs",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "bin.mjs",
    "dist"
  ],
  "scripts": {
    "build": "parcel build",
    "dev": "parcel watch --no-hmr",
    "prepack": "$npm_execpath run build"
  },
  "dependencies": {
    "@todone/core": "workspace:^",
    "@todone/default-plugins": "workspace:^",
    "@todone/types": "workspace:^",
    "chalk": "^5.2.0",
    "clipanion": "^3.2.0",
    "dotenv": "^16.0.3",
    "globby": "^13.1.3"
  },
  "devDependencies": {
    "@types/node": "^18",
    "parcel": "^2.8.3"
  },
  "targets": {
    "main": {
      "source": "src/index.ts"
    },
    "bin": {
      "source": "src/bin.ts",
      "optimize": false,
      "isLibrary": true,
      "outputFormat": "esmodule",
      "distDir": "dist"
    }
  }
}
