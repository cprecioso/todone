{
  "name": "todone",
  "version": "0.4.0",
  "author": "Carlos Precioso <npm@precioso.design>",
  "license": "ISC",
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": ">=16"
  },
  "bin": "dist/bin.mjs",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "parcel build",
    "dev": "parcel watch --no-hmr"
  },
  "dependencies": {
    "@todone/core": "workspace:^",
    "@todone/default-plugins": "workspace:^",
    "@todone/reporter-cli": "workspace:^",
    "clipanion": "^3.2.0-rc.11",
    "vinyl-fs": "^3.0.3"
  },
  "devDependencies": {
    "@todone/types": "workspace:^",
    "@types/node": "^16",
    "@types/vinyl-fs": "^2",
    "parcel": "^2.6.2"
  },
  "targets": {
    "main": {
      "source": "src/index.ts"
    },
    "bin": {
      "source": "src/bin.ts",
      "optimize": false,
      "isLibrary": true
    }
  }
}
