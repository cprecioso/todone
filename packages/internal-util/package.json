{
  "name": "@todone/internal-util",
  "version": "0.1.0",
  "type": "module",
  "author": "Carlos Precioso <npm@precioso.design>",
  "license": "ISC",
  "exports": {
    "./bool": {
      "types": "./dist/bool.d.ts",
      "default": "./dist/bool.js"
    },
    "./regex": {
      "types": "./dist/regex.d.ts",
      "default": "./dist/regex.js"
    },
    "./stream": {
      "types": "./dist/stream.d.ts",
      "default": "./dist/stream.js"
    },
    "./url": {
      "types": "./dist/url.d.ts",
      "default": "./dist/url.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "check": "tsc --noEmit -p .",
    "dev": "tsdown --watch"
  },
  "dependencies": {
    "@std/regexp": "jsr:^1.0.1",
    "@std/streams": "jsr:^1.0.9"
  },
  "devDependencies": {
    "@todone/build": "workspace:*",
    "tsdown": "^0.11.12",
    "typescript": "~5.8.3"
  },
  "sideEffects": false,
  "internal": true
}
