{
  "name": "@todone/plugin-caniuse",
  "version": "0.2.0",
  "type": "module",
  "description": "A todone plugin that will alert you when a feature is supported by your target browsers",
  "author": "Carlos Precioso <npm@precioso.design>",
  "license": "ISC",
  "engines": {
    "node": ">=22"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "check": "tsc --noEmit -p .",
    "dev": "tsdown --watch"
  },
  "dependencies": {
    "@std/assert": "jsr:^1.0.13",
    "@todone/internal-urlpattern": "workspace:^",
    "@todone/plugin": "workspace:^",
    "browserslist": "^4.24.5",
    "caniuse-lite": "^1.0.30001718"
  },
  "devDependencies": {
    "@todone/internal-build": "workspace:*",
    "@todone/types": "workspace:*",
    "@types/caniuse-lite": "^1.0.5",
    "@types/node": "^22.15.19",
    "tsdown": "^0.11.12",
    "typescript": "~5.8.3"
  },
  "sideEffects": false
}
