{
  "entryPointStrategy": "packages",
  "entryPoints": ["./packages/*"],
  "exclude": ["./packages/internal-*"],
  "includeVersion": true,
  "readme": "./docs.md",
  "packageOptions": {
    "includeVersion": true,
    "excludeInternal": true,
    "readme": "./docs.md"
  },
  "plugin": ["typedoc-plugin-dt-links", "typedoc-plugin-mdn-links"]
}
